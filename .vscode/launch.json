{
        // 启动调试的快捷键是F5
        "version": "0.2.0",
        "configurations": [
                {
                        "name": "STlink",
                        "cwd": "${workspaceRoot}",
                        "executable": "${command:cmake.launchTargetPath}", // 依赖cmake插件
                        "request": "launch",
                        "type": "cortex-debug",
                        "svdFile": "openocd/svd/STM32F103.svd", // 必须修改为自己的芯片型号, example: STM32F103.svd
                        "servertype": "openocd",
                        "configFiles": [
                                "interface/stlink.cfg",
                                "target/stm32f1x.cfg" // 必须修改为自己的芯片型号
                        ],
                        "runToEntryPoint": "main",
                        "rtos": "FreeRTOS",
                },
                {
                        "name": "STlink-attach",
                        "cwd": "${workspaceRoot}",
                        "executable": "${command:cmake.launchTargetPath}", // 依赖cmake插件
                        "request": "attach",
                        "type": "cortex-debug",
                        "svdFile": "openocd/svd/STM32F103.svd", // 必须修改为自己的芯片型号
                        "servertype": "openocd",
                        "configFiles": [
                                "interface/stlink.cfg",
                                "target/stm32f1x.cfg" // 必须修改为自己的芯片型号
                        ],
                },
                {
                        "name": "Cortex Debug",
                        "cwd": "${workspaceFolder}",
                        "executable": "./build/Debug/general-column-valve.elf",
                        "request": "launch",
                        "type": "cortex-debug",
                        "runToEntryPoint": "main",
                        "servertype": "openocd",
                        "configFiles": [
                                "interface/stlink.cfg",
                                "target/stm32f1x.cfg"
                        ],
                        "armToolchainPath": "D:/toolchain/arm-gnu-toolchain-13.3.rel1/bin",
                        "gdbPath": "D:/toolchain/arm-gnu-toolchain-13.3.rel1/bin/arm-none-eabi-gdb.exe",
                        "interface": "swd",
                        "svdPath": "D:\\toolchain\\STM32CubeCLT_1.18.0\\STMicroelectronics_CMSIS_SVD",
                        "showDevDebugOutput": "raw",
                }
        ],
}